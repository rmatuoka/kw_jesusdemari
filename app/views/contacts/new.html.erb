					<a href='javascript:void(0)'><li id='novidades'><li></a>
					<a href='javascript:void(0)'><li id='sustentabilidade'><li></a>
					<a href='javascript:void(0)'><li id='contato'><li></a>
				</ul>
			</div>
		</div>
		<div id='article'>
			<div id='article-content'>
				<div id='top-contacts'>
					<h1>CONTATO</h1>
				</div>
				<div class='clear'></div>
				<div id='contacts-article'>
					<%= link_to image_tag('btn_voltar.jpg', :height=>'46', :width=>'107', :alt=>'Castelatto', :title=>'Castelatto'), 'www.uol.com.br', :id=>'btn_voltar' %>
					
					<p>Telefone: (11) 4416-7007 - Financeiro<br><br>
					
